    <!-- http://link477.com/ds/pizza#american -->

    <owl:Class rdf:about="http://link477.com/ds/pizza#american">
        <rdfs:subClassOf rdf:resource="http://link477.com/ds/pizza#namedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://link477.com/ds/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://link477.com/ds/pizza#mozzarella"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://link477.com/ds/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://link477.com/ds/pizza#pepperoni"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://link477.com/ds/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://link477.com/ds/pizza#tomato"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://link477.com/ds/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
