@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pizza: <http://www.co-ode.org/ontologies/pizza/pizza.owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tef: <http://link477.com/ds/pizza#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://link477.com/ds/pizza> a owl:Ontology ;
    rdfs:label "Pizza Knowledge Graph"^^xsd:string ;
    tef:creator "Creator"^^xsd:string ;
    rdfs:comment "Pizza Knowledge Graph, created by Thomas Fishwick, 2023-02-11"^^xsd:string ;
    owl:imports <http://www.co-ode.org/ontologies/pizza/2.0.0> .

tef:address a owl:DatatypeProperty ;
    rdfs:label "Restaurant Address"^^xsd:string ;
    rdfs:comment "The address of the pizza restaurant"^^xsd:string ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

tef:breakfastPizza a owl:Class ;
    rdfs:label "Breakfast Pizza"^^xsd:string ;
    rdfs:comment "Breakfast Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:germanSausage ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:egg ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        tef:namedPizza .

tef:burger a owl:Class ;
    rdfs:label "Burger"^^xsd:string ;
    rdfs:comment "Burger"^^xsd:string ;
    rdfs:subClassOf tef:pizzaBase .

tef:category a owl:DatatypeProperty ;
    rdfs:label "Category"^^xsd:string ;
    rdfs:comment "The various categories of the restaurant"^^xsd:string ;
    rdfs:range xsd:string .

tef:cheesyPizza a owl:Class ;
    rdfs:label "Cheesy Pizza"^^xsd:string ;
    rdfs:comment "Pizza with just cheese on it"^^xsd:string ;
    rdfs:subClassOf tef:pizzaType ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( tef:pizza [ a owl:Restriction ;
                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                        owl:onClass tef:cheese ;
                        owl:onProperty tef:hasTopping ] ) ] .

tef:cityHasRestaurant a owl:ObjectProperty ;
    rdfs:label "City has Restaurant"^^xsd:string ;
    rdfs:comment "The restaurants a city has"^^xsd:string ;
    rdfs:domain tef:city ;
    rdfs:range tef:restaurant ;
    owl:inverseOf tef:restaurantInCity .

tef:cityLocatedIn a owl:ObjectProperty ;
    rdfs:label "City located in"^^xsd:string ;
    rdfs:comment "The province a city is in"^^xsd:string ;
    rdfs:domain tef:city ;
    rdfs:range tef:province ;
    owl:inverseOf tef:hasCity .

tef:creator a owl:AnnotationProperty ;
    rdfs:label "Thomas Fishwick"@en ;
    rdfs:comment "Created by Thomas Fishwick, 2023-02-11"@en .

tef:curry a owl:Class ;
    rdfs:label "Curry"^^xsd:string ;
    rdfs:comment "Curry Sauce"^^xsd:string ;
    rdfs:subClassOf tef:pizzaTopping .

tef:dateAdded a owl:DatatypeProperty ;
    rdfs:label "Date added"^^xsd:string ;
    rdfs:comment "The date the item was added to the database"^^xsd:string ;
    rdfs:range xsd:dateTime .

tef:dateUpdated a owl:DatatypeProperty ;
    rdfs:label "Date added"^^xsd:string ;
    rdfs:comment "The date the item was updated in the database"^^xsd:string ;
    rdfs:range xsd:dateTime .

tef:doughSticks a owl:Class ;
    rdfs:label "Dough Sticks"^^xsd:string ;
    rdfs:comment "Dough Sticks"^^xsd:string ;
    rdfs:subClassOf tef:menuItem .

tef:fontina a owl:Class ;
    rdfs:label "Fontina"^^xsd:string ;
    rdfs:comment "Fontina"^^xsd:string ;
    rdfs:subClassOf tef:cheese .

tef:glutenFreePizza a owl:Class ;
    rdfs:label "Gluten Free Pizza"^^xsd:string ;
    rdfs:comment "Gluten Free Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasBase ;
            owl:someValuesFrom tef:glutenFreeBase ],
        tef:pizzaType .

tef:hasMenuItem a owl:ObjectProperty ;
    rdfs:label "Has Menu Item"^^xsd:string ;
    rdfs:comment "The menu items a restaurant has"^^xsd:string ;
    rdfs:domain tef:restaurant ;
    rdfs:range tef:menuItem ;
    owl:inverseOf tef:servedInRestaurant .

tef:hasProvince a owl:ObjectProperty ;
    rdfs:label "Country Has Province"^^xsd:string ;
    rdfs:comment "The provinces within a country"^^xsd:string ;
    rdfs:domain tef:country ;
    rdfs:range tef:province ;
    owl:inverseOf tef:provinceLocatedIn .

tef:id a owl:DatatypeProperty ;
    rdfs:label "ID"^^xsd:string ;
    rdfs:comment "The ID of the menu item"^^xsd:string ;
    rdfs:range xsd:string .

tef:interestingPizza a owl:Class ;
    rdfs:label "Interesting Pizza"^^xsd:string ;
    rdfs:comment "A pizza with at least 3 toppings"^^xsd:string ;
    rdfs:subClassOf tef:pizzaType ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( tef:pizza [ a owl:Restriction ;
                        owl:minCardinality "3"^^xsd:nonNegativeInteger ;
                        owl:onProperty tef:hasTopping ] ) ] .

tef:isBaseOf a owl:ObjectProperty ;
    rdfs:label "Is Base Of"^^xsd:string ;
    rdfs:comment "The pizzas a base makes"^^xsd:string ;
    rdfs:domain tef:pizzaBase ;
    rdfs:range tef:menuItem ;
    rdfs:subPropertyOf tef:isIngredientOf ;
    owl:equivalentProperty pizza:isBaseOf .

tef:isToppingOf a owl:ObjectProperty ;
    rdfs:label "Is Topping Of"^^xsd:string ;
    rdfs:comment "The pizzas a topping makes up"^^xsd:string ;
    rdfs:domain tef:pizzaTopping ;
    rdfs:range tef:menuItem ;
    rdfs:subPropertyOf tef:isIngredientOf ;
    owl:equivalentProperty pizza:isToppingOf .

tef:keys a owl:DatatypeProperty ;
    rdfs:label "The keys of the item"^^xsd:string ;
    rdfs:comment "The keys of the menu item"^^xsd:string ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

tef:latitude a owl:DatatypeProperty ;
    rdfs:label "Latitude"^^xsd:string ;
    rdfs:comment "The latitude of the restaurant"^^xsd:string ;
    rdfs:range xsd:decimal .

tef:longitude a owl:DatatypeProperty ;
    rdfs:label "Longitude"^^xsd:string ;
    rdfs:comment "The longitude of the restaurant"^^xsd:string ;
    rdfs:range xsd:decimal .

tef:meatPizza a owl:Class ;
    rdfs:label "Meat Pizza"^^xsd:string ;
    rdfs:comment "A pizza with meat on it"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:meat ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        tef:namedPizza .

tef:menuPageURL a owl:DatatypeProperty ;
    rdfs:label "Menu Page URL"^^xsd:string ;
    rdfs:comment "The menu page URL"^^xsd:string ;
    rdfs:range xsd:anyURI .

tef:menus.amountMax a owl:DatatypeProperty ;
    rdfs:label "Max amount of the menu"^^xsd:string ;
    rdfs:comment "The maximum amount on the menu"^^xsd:string ;
    rdfs:range xsd:decimal .

tef:menus.amountMin a owl:DatatypeProperty ;
    rdfs:label "Min amount of the menu"^^xsd:string ;
    rdfs:comment "The minimum amount on the menu"^^xsd:string ;
    rdfs:range xsd:decimal .

tef:menus.currency a owl:DatatypeProperty ;
    rdfs:label "Menu currency"^^xsd:string ;
    rdfs:comment "The currency of the menu"^^xsd:string ;
    rdfs:range xsd:string .

tef:menus.dateSeen a owl:DatatypeProperty ;
    rdfs:label "Menu date seen"^^xsd:string ;
    rdfs:comment "The date times that the menu was seen"^^xsd:string ;
    rdfs:range xsd:dateTime .

tef:menus.description a owl:DatatypeProperty ;
    rdfs:label "Menu description"^^xsd:string ;
    rdfs:comment "The description/ingrediants of the menu item"^^xsd:string ;
    rdfs:range xsd:string .

tef:menus.name a owl:DatatypeProperty ;
    rdfs:label "Menu item name"^^xsd:string ;
    rdfs:comment "The name of the menu item"^^xsd:string ;
    rdfs:range xsd:string .

tef:pizzaBurger a owl:Class ;
    rdfs:label "Pizza Burger"^^xsd:string ;
    rdfs:comment "Pizza Burger"^^xsd:string ;
    rdfs:subClassOf tef:pizzaType .

tef:pizzaRoll a owl:Class ;
    rdfs:label "Pizza Roll"^^xsd:string ;
    rdfs:comment "Pizza Roll"^^xsd:string ;
    rdfs:subClassOf tef:pizzaType .

tef:pizzaSauce a owl:Class ;
    rdfs:label "Pizza Sauce"^^xsd:string ;
    rdfs:comment "Pizza Sauce"^^xsd:string ;
    rdfs:subClassOf tef:pizzaTopping .

tef:pizzaSize a owl:DatatypeProperty ;
    rdfs:label "Pizza size"^^xsd:string ;
    rdfs:comment "The size of the pizza in inches"^^xsd:string ;
    rdfs:range xsd:integer .

tef:pizzaSlice a owl:Class ;
    rdfs:label "Pizza Slice"^^xsd:string ;
    rdfs:comment "Slice of pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasBase ;
            owl:someValuesFrom tef:pizzaSliceBase ],
        tef:pizzaType .

tef:pizzaSub a owl:Class ;
    rdfs:label "Pizza Sub"^^xsd:string ;
    rdfs:comment "A pizza sub"^^xsd:string ;
    rdfs:subClassOf tef:pizzaType .

tef:pizzaSubBase a owl:Class ;
    rdfs:label "Pizza Sub Base"^^xsd:string ;
    rdfs:comment "A base for a pizza sub"^^xsd:string ;
    rdfs:subClassOf tef:pizzaBase .

tef:pizzaiola a owl:Class ;
    rdfs:label "Pizzaiola"^^xsd:string ;
    rdfs:comment "Pizzaiola, meat cooked pizza style"^^xsd:string ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/Carne_pizzaiola"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:basil ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:garlic ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:oregano ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:meat ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:oil ],
        tef:menuItem .

tef:pizzetta a owl:Class ;
    rdfs:label "Pizzetta"^^xsd:string ;
    rdfs:comment "Small pizza, ranges in size from three inches to a small personal-sized pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasBase ;
            owl:someValuesFrom tef:smallPizzaBase ],
        tef:namedPizza .

tef:postalCode a owl:DatatypeProperty ;
    rdfs:label "Post code"^^xsd:string ;
    rdfs:comment "Post code of the restaurant"^^xsd:string ;
    rdfs:range xsd:string .

tef:priceRangeCurrency a owl:DatatypeProperty ;
    rdfs:label "Price Range Currency"^^xsd:string ;
    rdfs:comment "The price range of the menu item's currency"^^xsd:string ;
    rdfs:range xsd:string .

tef:priceRangeMax a owl:DatatypeProperty ;
    rdfs:label "Price range max"^^xsd:string ;
    rdfs:comment "Maximum price range"^^xsd:string ;
    rdfs:range xsd:integer .

tef:priceRangeMin a owl:DatatypeProperty ;
    rdfs:label "Price range min"^^xsd:string ;
    rdfs:comment "Minimum price range of the menu item"^^xsd:string ;
    rdfs:range xsd:integer .

tef:primaryCategories a owl:DatatypeProperty ;
    rdfs:label "Primary Categories"^^xsd:string ;
    rdfs:comment "The primary categroies of the restaurant"^^xsd:string ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

tef:restaurantName a owl:DatatypeProperty ;
    rdfs:label "Restaurant Name"^^xsd:string ;
    rdfs:comment "The name of the restaurant"^^xsd:string ;
    rdfs:range xsd:string .

tef:ricotta a owl:Class ;
    rdfs:label "Ricotta"^^xsd:string ;
    rdfs:comment "Ricotta"^^xsd:string ;
    rdfs:subClassOf tef:cheese .

tef:stracchino a owl:Class ;
    rdfs:label "Stracchino"^^xsd:string ;
    rdfs:comment "Stracchino"^^xsd:string ;
    rdfs:subClassOf tef:cheese .

tef:vegetarianPizza a owl:Class ;
    rdfs:label "Vegetarian Pizza"^^xsd:string ;
    rdfs:comment "Pizzas suitable for vegetarians"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Class ;
            owl:complementOf [ a owl:Restriction ;
                    owl:onProperty tef:hasTopping ;
                    owl:someValuesFrom tef:seafood ] ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        tef:namedPizza .

tef:american a owl:Class ;
    rdfs:label "American"^^xsd:string ;
    rdfs:comment "American Pizza/pepperoni"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:pepperoni ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        tef:namedPizza ;
    owl:equivalentClass pizza:American .

tef:americanHot a owl:Class ;
    rdfs:label "American Hot"^^xsd:string ;
    rdfs:comment "A spicy American pizza with chilli peppers"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:pepperoni ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:hotGreenPepper ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:jalapenoPepper ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        tef:namedPizza ;
    owl:equivalentClass pizza:AmericanHot .

tef:anchovyPizza a owl:Class ;
    rdfs:label "Anchovy Pizza"^^xsd:string ;
    rdfs:comment "Anchovy Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:anchovies ],
        tef:namedPizza .

tef:arugula a owl:Class ;
    rdfs:label "Arugula"^^xsd:string ;
    rdfs:comment "Something that looks like rocket"^^xsd:string ;
    rdfs:subClassOf tef:pizzaTopping .

tef:asparagus a owl:Class ;
    rdfs:label "Asparagus"^^xsd:string ;
    rdfs:comment "A vegetable"^^xsd:string ;
    rdfs:subClassOf tef:pizzaTopping ;
    owl:equivalentClass pizza:AsparagusTopping .

tef:aubergine a owl:Class ;
    rdfs:label "Aubergine"^^xsd:string ;
    rdfs:comment "Aubergine/eggplant"^^xsd:string ;
    rdfs:subClassOf tef:pizzaTopping .

tef:auberginePizza a owl:Class ;
    rdfs:label "Aubergine Pizzza"^^xsd:string ;
    rdfs:comment "Aubergine Pizzza/Eggplant"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:aubergine ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        tef:namedPizza .

tef:baconCheeseBurgerPizza a owl:Class ;
    rdfs:label "Bacon Cheeseburger Pizza"^^xsd:string ;
    rdfs:comment "Bacon Cheeseburger Pizza"^^xsd:string ;
    rdfs:seeAlso "https://www.papajohns.com/order/menu/pizza/bacon-cheddarburger#:~:text=Our%20fan%20favorite%20is%20better,on%20Pan%20or%20Large%20crust."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:pickle ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:burgerSauce ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:bacon ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:cheddar ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        tef:namedPizza .

tef:bakedPotatoPizza a owl:Class ;
    rdfs:label "Baked Potato Pizza"^^xsd:string ;
    rdfs:comment "Baked Potato Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:bacon ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:onion ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:sourCream ],
        tef:namedPizza .

tef:bbqPizza a owl:Class ;
    rdfs:label "BBQ Pizza"^^xsd:string ;
    rdfs:comment "BBQ Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:corn ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:bacon ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:scallions ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:onion ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:bbqSauce ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        tef:namedPizza .

tef:bbqSauce a owl:Class ;
    rdfs:label "BBQ Sauce"^^xsd:string ;
    rdfs:comment "BBQ Sauce"^^xsd:string ;
    rdfs:subClassOf tef:pizzaTopping .

tef:beef a owl:Class ;
    rdfs:label "Beef"^^xsd:string ;
    rdfs:comment "Beef"^^xsd:string ;
    rdfs:subClassOf tef:meat .

tef:bianca a owl:Class ;
    rdfs:label "Pizza Bianca"^^xsd:string ;
    rdfs:comment "White pizza. No tomato. Olive oil, salt, occasionally herbs"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:oil ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        tef:namedPizza .

tef:biancaFiorentinaPizza a owl:Class ;
    rdfs:label "Bianca Fiorentina Pizza"^^xsd:string ;
    rdfs:comment "Bianca Fiorentina Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:garlic ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:parmezan ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:olives ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:spinach ],
        tef:namedPizza .

tef:bltPizza a owl:Class ;
    rdfs:label "BLT Pizza"^^xsd:string ;
    rdfs:comment "BLT Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:bacon ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:lettuce ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        tef:namedPizza .

tef:blueCheese a owl:Class ;
    rdfs:label "Blue Cheese"^^xsd:string ;
    rdfs:comment "Blue Cheese"^^xsd:string ;
    rdfs:subClassOf tef:cheese .

tef:bruschettaPizza a owl:Class ;
    rdfs:label "Bruschetta Pizza"^^xsd:string ;
    rdfs:comment "Bruschetta Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:mozzarella ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:parmezan ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:sundriedTomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:garlic ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:chicken ],
        tef:namedPizza .

tef:buffaloChickenPizza a owl:Class ;
    rdfs:label "Buffalo Chicken Pizza"^^xsd:string ;
    rdfs:comment "Buffalo Chicken Pizza"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:tomato ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:chicken ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:blueCheese ],
        [ a owl:Restriction ;
            owl:onProperty tef:hasTopping ;
            owl:someValuesFrom tef:buffaloSauce ],
