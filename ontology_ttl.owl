@prefix : <http://link477.com/ds/pizza#> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix tef: <http://link477.com/ds/pizza#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://link477.com/ds/pizza> .

<http://link477.com/ds/pizza> rdf:type owl:Ontology ;
                               tef:creator "Creator"^^xsd:string ;
                               rdfs:comment "Pizza Knowledge Graph, created by Thomas Fishwick, 2023-02-11"^^xsd:string ;
                               rdfs:label "Pizza Knowledge Graph"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://link477.com/ds/pizza#creator
tef:creator rdf:type owl:AnnotationProperty ;
            rdfs:comment "Created by Thomas Fishwick, 2023-02-11"@en ;
            rdfs:label "Thomas Fishwick"@en .


#################################################################
#    Object Properties
#################################################################

###  http://link477.com/ds/pizza#cityHasRestaurant
tef:cityHasRestaurant rdf:type owl:ObjectProperty ;
                      owl:inverseOf tef:restaurantInCity ;
                      rdfs:domain tef:city ;
                      rdfs:range tef:restaurant ;
                      rdfs:comment "The restaurants a city has"^^xsd:string ;
                      rdfs:label "City has Restaurant"^^xsd:string .


###  http://link477.com/ds/pizza#cityLocatedIn
tef:cityLocatedIn rdf:type owl:ObjectProperty ;
                  owl:inverseOf tef:hasCity ;
                  rdfs:domain tef:city ;
                  rdfs:range tef:province ;
                  rdfs:comment "The province a city is in"^^xsd:string ;
                  rdfs:label "City located in"^^xsd:string .


###  http://link477.com/ds/pizza#hasBase
tef:hasBase rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf tef:hasIngredient ;
            rdfs:domain tef:pizza ;
            rdfs:range tef:pizzaBase ;
            rdfs:comment "The type of base a pizza has"^^xsd:string ;
            rdfs:label "Has Base"^^xsd:string .


###  http://link477.com/ds/pizza#hasCity
tef:hasCity rdf:type owl:ObjectProperty ;
            rdfs:domain tef:province ;
            rdfs:range tef:city ;
            rdfs:comment "The cities a province has"^^xsd:string ;
            rdfs:label "Province has city"^^xsd:string .


###  http://link477.com/ds/pizza#hasIngredient
tef:hasIngredient rdf:type owl:ObjectProperty ;
                  owl:inverseOf tef:isIngredientOf ;
                  rdf:type owl:TransitiveProperty ;
                  rdfs:domain tef:menuItem ;
                  rdfs:range tef:ingredients ;
                  rdfs:comment "The ingredients of a menu item"^^xsd:string ;
                  rdfs:label "Has Ingredient"^^xsd:string .


###  http://link477.com/ds/pizza#hasMenuItem
tef:hasMenuItem rdf:type owl:ObjectProperty ;
                owl:inverseOf tef:servedInRestauant ;
                rdfs:domain tef:restaurant ;
                rdfs:range tef:menuItem ;
                rdfs:comment "The menu items a restaurant has"^^xsd:string ;
                rdfs:label "Has Menu Item"^^xsd:string .


###  http://link477.com/ds/pizza#hasProvince
tef:hasProvince rdf:type owl:ObjectProperty ;
                owl:inverseOf tef:provinceLocatedIn ;
                rdfs:domain tef:country ;
                rdfs:range tef:province ;
                rdfs:comment "The provinces within a country"^^xsd:string ;
                rdfs:label "Country Has Province"^^xsd:string .


###  http://link477.com/ds/pizza#hasTopping
tef:hasTopping rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf tef:hasIngredient ;
               rdfs:domain tef:pizza ;
               rdfs:range tef:pizzaTopping ;
               rdfs:comment "The toppings a pizza has"^^xsd:string ;
               rdfs:label "Has Topping"^^xsd:string .


###  http://link477.com/ds/pizza#isBaseOf
tef:isBaseOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf tef:isIngredientOf ;
             rdfs:domain tef:pizzaBase ;
             rdfs:range tef:menuItem ;
             rdfs:comment "The pizzas a base makes"^^xsd:string ;
             rdfs:label "Is Base Of"^^xsd:string .


###  http://link477.com/ds/pizza#isIngredientOf
tef:isIngredientOf rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain tef:ingredients ;
                   rdfs:range tef:menuItem ;
                   rdfs:comment "The food item an ingredient makes"^^xsd:string ;
                   rdfs:label "Is Ingredient Of"^^xsd:string .


###  http://link477.com/ds/pizza#isToppingOf
tef:isToppingOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf tef:isIngredientOf ;
                rdfs:domain tef:pizzaTopping ;
                rdfs:range tef:menuItem ;
                rdfs:comment "The pizzas a topping makes up"^^xsd:string ;
                rdfs:label "Is Topping Of"^^xsd:string .


###  http://link477.com/ds/pizza#provinceLocatedIn
tef:provinceLocatedIn rdf:type owl:ObjectProperty ;
                      rdfs:domain tef:province ;
                      rdfs:range tef:country ;
                      rdfs:comment "The country a province is within"^^xsd:string ;
                      rdfs:label "Province Located In"^^xsd:string .


###  http://link477.com/ds/pizza#restaurantInCity
tef:restaurantInCity rdf:type owl:ObjectProperty ;
                     rdfs:domain tef:restaurant ;
                     rdfs:range tef:city ;
                     rdfs:comment "The city a restaurant is in"^^xsd:string ;
                     rdfs:label "Restaurant In City"^^xsd:string .


###  http://link477.com/ds/pizza#servedInRestauant
tef:servedInRestauant rdf:type owl:ObjectProperty ;
                      rdfs:domain tef:menuItem ;
                      rdfs:range tef:restaurant ;
                      rdfs:comment "The restaurant a menu item is served in"^^xsd:string ;
                      rdfs:label "Served In Restaurant"^^xsd:string .


