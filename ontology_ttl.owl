###  http://link477.com/ds/pizza#menus.amountMax
tef:menus.amountMax rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "The maximum amount on the menu"^^xsd:string ;
                    rdfs:label "Max amount of the menu"^^xsd:string .


###  http://link477.com/ds/pizza#menus.amountMin
tef:menus.amountMin rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "The minimum amount on the menu"^^xsd:string ;
                    rdfs:label "Min amount of the menu"^^xsd:string .


###  http://link477.com/ds/pizza#menus.currency
tef:menus.currency rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:string ;
                   rdfs:comment "The currency of the menu"^^xsd:string ;
                   rdfs:label "Menu currency"^^xsd:string .


###  http://link477.com/ds/pizza#menus.dateSeen
tef:menus.dateSeen rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:dateTime ;
                   rdfs:comment "The date times that the menu was seen"^^xsd:string ;
                   rdfs:label "Menu date seen"^^xsd:string .


###  http://link477.com/ds/pizza#menus.description
tef:menus.description rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:string ;
                      rdfs:comment "The description/ingrediants of the menu item"^^xsd:string ;
                      rdfs:label "Menu description"^^xsd:string .


###  http://link477.com/ds/pizza#menus.name
tef:menus.name rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               rdfs:comment "The name of the menu item"^^xsd:string ;
               rdfs:label "Menu item name"^^xsd:string .


###  http://link477.com/ds/pizza#pizzaSize
tef:pizzaSize rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:integer ;
              rdfs:comment "The size of the pizza in inches"^^xsd:string ;
              rdfs:label "Pizza size"^^xsd:string .


###  http://link477.com/ds/pizza#postalCode
tef:postalCode rdf:type owl:DatatypeProperty ;
