            rdfs:range xsd:string ;
            rdfs:comment "The address of the pizza restaurant"^^xsd:string ;
            rdfs:label "Restaurant Address"^^xsd:string .


###  http://link477.com/ds/pizza#category
tef:category rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:comment "The various categories of the restaurant"^^xsd:string ;
             rdfs:label "Category"^^xsd:string .


###  http://link477.com/ds/pizza#dateAdded
tef:dateAdded rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:dateTime ;
              rdfs:comment "The date the item was added to the database"^^xsd:string ;
              rdfs:label "Date added"^^xsd:string .


###  http://link477.com/ds/pizza#dateUpdated
tef:dateUpdated rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "The date the item was updated in the database"^^xsd:string ;
                rdfs:label "Date added"^^xsd:string .


###  http://link477.com/ds/pizza#id
tef:id rdf:type owl:DatatypeProperty ;
       rdfs:range xsd:string ;
       rdfs:comment "The ID of the menu item"^^xsd:string ;
       rdfs:label "ID"^^xsd:string .


###  http://link477.com/ds/pizza#keys
tef:keys rdf:type owl:DatatypeProperty ;
