###  http://link477.com/ds/pizza#primaryCategories
tef:primaryCategories rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:range xsd:string ;
                      rdfs:comment "The primary categroies of the restaurant"^^xsd:string ;
                      rdfs:label "Primary Categories"^^xsd:string .


###  http://link477.com/ds/pizza#resaurantName
tef:resaurantName rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:string ;
                  rdfs:comment "The name of the restaurant"^^xsd:string ;
                  rdfs:label "Restaurant Name"^^xsd:string .


#################################################################
#    Classes
#################################################################

###  http://link477.com/ds/pizza#american
tef:american rdf:type owl:Class ;
             rdfs:subClassOf tef:namedPizza ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty tef:hasTopping ;
                               owl:someValuesFrom tef:mozzarella
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty tef:hasTopping ;
                               owl:someValuesFrom tef:pepperoni
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty tef:hasTopping ;
                               owl:someValuesFrom tef:tomato
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty tef:hasTopping ;
                               owl:allValuesFrom [ rdf:type owl:Class ;
                                                   owl:unionOf ( tef:mozzarella
                                                                 tef:pepperoni
                                                                 tef:tomato
                                                               )
                                                 ]
                             ] ;
             rdfs:comment "American Pizza/pepperoni"^^xsd:string ;
             rdfs:label "American"^^xsd:string .


###  http://link477.com/ds/pizza#americanHot
tef:americanHot rdf:type owl:Class ;
                rdfs:subClassOf tef:namedPizza ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty tef:hasTopping ;
                                  owl:someValuesFrom tef:hotGreenPepper
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty tef:hasTopping ;
                                  owl:someValuesFrom tef:jalapenoPepper
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty tef:hasTopping ;
                                  owl:someValuesFrom tef:mozzarella
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty tef:hasTopping ;
                                  owl:someValuesFrom tef:pepperoni
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty tef:hasTopping ;
                                  owl:someValuesFrom tef:tomato
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty tef:hasTopping ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( tef:hotGreenPepper
                                                                    tef:jalapenoPepper
                                                                    tef:mozzarella
                                                                    tef:pepperoni
                                                                    tef:tomato
                                                                  )
                                                    ]
                                ] ;
                rdfs:comment "A spicy American pizza with chilli peppers"^^xsd:string ;
                rdfs:label "American Hot"^^xsd:string .


