###  http://link477.com/ds/pizza#bianca
tef:bianca rdf:type owl:Class ;
           rdfs:subClassOf tef:namedPizza ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty tef:hasTopping ;
                             owl:someValuesFrom tef:mozzarella
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty tef:hasTopping ;
                             owl:someValuesFrom tef:oil
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty tef:hasTopping ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( tef:mozzarella
                                                               tef:oil
                                                             )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty tef:hasTopping ;
                             owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                             owl:onClass tef:tomato
                           ] ;
           rdfs:comment "White pizza. No tomato. Olive oil, salt, occasionally herbs"^^xsd:string ;
           rdfs:label "Pizza Bianca"^^xsd:string .


